    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@auth0/angular-jwt": "^5.2.0",
    "@sweetalert2/ngx-sweetalert2": "^12.3.0",
    "@types/jquery": "^3.5.32",
    "@types/slick-carousel": "^1.6.40",
    "ag-grid-community": "^31.3.2",
    "angular2-multiselect-dropdown": "^10.0.0",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.5",
    "chartjs-plugin-datalabels": "^2.2.0",
    "file-saver": "^2.0.5",
    "jquery": "^3.7.1",
    "jspdf": "^2.5.2",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
