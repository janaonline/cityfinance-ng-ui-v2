{
  "name": "cityfinance-ng-ui-v2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "local": "ng serve --configuration=local --port 4300",
    "host": "ng serve --configuration=staging --host local.cityfinance.in --port 4300",
    "net": "ng serve --configuration=staging --host 0.0.0.0 --port 4300",
    "staging": "ng serve --configuration=staging --port 4300",
    "dev": "ng serve --configuration=development --port 4300",
    "start": "ng serve",
    "build": "ng build --base-href /fc/",
    "build1": "ng build",
    "build:stats": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.html\"",
    "sourceMap": "ng build --configuration production --source-map --stats-json",
    "analyze": "webpack-bundle-analyzer ./dist/cityfinance-ng-ui-v2/stats.json",
    "sm": "source-map-explorer dist/cityfinance-ng-ui-v2/browser/main.*.js",
    "analyze-esbuild": "ng build && esbuild dist/cityfinance-ng-ui-v2/browser/main.*.js --bundle --metafile=dist/cityfinance-ng-ui-v2/browser/meta.json --outfile=out.js",
    "analyze-esbuil1": "esbuild dist/cityfinance-ng-ui-v2/browser/main-*.js --bundle --outdir=analyzed.js --metafile=meta.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
